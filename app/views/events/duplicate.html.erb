<h1>複製</h1>
<div class="row">
  <div class="col-md-6">
    <%= form_for(@event, url: events_path, method: :post) do |f| %>
    <%= f.hidden_field :id %>
    <% @f = f %>
    <%= render 'form' %>
    <% end %>
  </div>
</div>
<hr/>
<div class="row">
  <div class="col-md-6">
    <%= link_to '一覧に戻る', events_path, class: 'btn btn-default' %>
  </div>
</div>
