<div class="row">
  <div class="col-md-6">
    <h2>開催済み</h2>
  </div>
  <div class="col-md-6 text-right">
    <%= link_to 'トップページ', root_path, class: 'btn btn-default' %>
  </div>
</div>
<div class="row">
  <% @ends.each do |col| %>
    <div class="col-sm-4">
      <% col.each_with_index do |event,i| %>
        <%= render partial: 'event', locals: {event: event,index: i} %>
      <% end %>
    </div>
  <% end %>
</div>
<div class="row">
  <div class="col-md-12 text-right">
    <%= link_to 'トップページ', root_path, class: 'btn btn-default' %>
  </div>
</div>
