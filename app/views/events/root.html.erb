<h1>Listing events</h1>

<table class="table">
  <thead>
    <tr>
      <th>User</th>
      <th>Title</th>
      <th>Opendate</th>
      <th>Visible</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.user_id %></td>
        <td><%= link_to event.title, event %></td>
        <td><%= event.opendate %></td>
        <td><%= event.visible %></td>
      </tr>
    <% end %>
  </tbody>
</table>
